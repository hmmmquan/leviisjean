                        <div class="asker-name"><p class="asker">{Asker}:</p><p>{Question}</p></div>
                    </div>
                    
                    {block:NotReblog}
                    
                    {block:Date}
                    <div class="source-container">
                        <div class="source-avatar"><a href="{Permalink}"><img src="{PortraitURL-64}"></a></div>
                        <div class="source-username"><a href="{Permalink}">{Name}</a></div>
                    </div>
                    {/block:Date}
                    
                    <div class="caption {block:Date}video{/block:Date}">{Answer}</div>
                    
                    {/block:NotReblog}
                    
                    {block:RebloggedFrom}
                    
                        {block:Answerer}
                        
                            {block:Date}
                            <div class="source-container">
                                <div class="source-avatar"><a href="{ReblogRootURL}"><img src="{ReblogRootPortraitURL-64}"></a></div>
                                <div class="source-username"><a href="{ReblogRootURL}">@{ReblogRootName}</a>{block:IsDeactivated} (deactivated){/block:IsDeactivated}</div>
                            </div>
                            {/block:Date}
                            
                            <div class="caption {block:Date}video{/block:Date}">{Answer}</div>
                        {/block:Answerer}
                            
                        {block:Reblogs}
                            
                            {block:Date}
                            <div class="source-container" {block:IsNotOriginalEntry}style="margin-top: 1.5em; border-top: 1px solid {color:Post Border};"{/block:IsNotOriginalEntry}>
                                <div class="source-avatar"><a href="{Permalink}"><img src="{PortraitURL-64}"></a></div>
                                <div class="source-username"><a href="{Permalink}">{Username}</a>{block:IsDeactivated} (deactivated){/block:IsDeactivated}</div>
                            </div>
                            {/block:Date}
                            
                            <div class="caption {block:Date}video{/block:Date}">{Body}</div> 
                            
                        {/block:Reblogs}
                    
                    {/block:RebloggedFrom}
                
                {/block:Answer} 
                
                {block:Caption}
                
                    {block:NotReblog}
                
                    <div class="caption {block:Date}video{/block:Date}">{Caption}</div>
                    {/block:NotReblog}
                    
                    {block:RebloggedFrom}{block:Reblogs}
                    
                    {block:Date}
                    <div class="source-container" {block:isOriginalEntry}style="display:none;"{/block:isOriginalEntry}{block:IsNotOriginalEntry}style="margin-top: 1.5em; border-top: 1px solid {color:Post Border};"{/block:IsNotOriginalEntry}>
                        <div class="source-avatar"><a href="{Permalink}"><img src="{PortraitURL-64}"></a></div>
                        <div class="source-username"><a href="{Permalink}">{Username}</a>{block:IsDeactivated} {block:IfJapanese}(無効化された){/block:IfJapanese}{block:IfNotJapanese}(deactivated){/block:IfNotJapanese}{/block:IsDeactivated}</div>
                    </div>
                    {/block:Date}
                    
                    <div class="caption {block:Date}video{/block:Date}">
                        {Body}
                    </div>
                    
                    {/block:Reblogs}{/block:RebloggedFrom}
                    
                {/block:Caption}
                
                {block:ContentSource} 
                    <p class="entry-source">
                        (<strong>Source:</strong> <a href="{SourceURL}">{SourceTitle}</a>)
                    </p>
                {/block:ContentSource}
                
                {block:HasTags}
                    <div class="entry-tags">
                        {block:Tags}<a href="{TagURL}">#{Tag}</a>{/block:Tags}
                    </div>
                {/block:HasTags}
                
                <div class="entry-info">
                    <div class="entry-info-left">
                        <a href="{Permalink}" title="Permalink">{NoteCountWithLabel}</a> <span class="time-ago">{TimeAgo}</span>
                    </div>
                    {block:Date}
                    <div class="entry-info-right">
                        <div class="interact-button"><a href="https://www.tumblr.com/{Name}/{PostID}/" title="Open in Dashboard"><i class="ph-bold ph-eye"></i></a></div>
                        <div class="interact-button" title="Reblog"><i class="ph-bold ph-arrows-counter-clockwise"></i> {ReblogButton size="18"}</div>
                        <div class="interact-button" title="Like"><i class="ph-fill ph-heart"></i> {LikeButton}</div> 
                    </div>
                    {/block:Date}
                </div>
                
                {block:PostNotes}
                <div class="entry-comments">
                    {PostNotes-64}
                </div>
                {/block:PostNotes}
                
            </div>
            
            {/block:Posts}
            
        </section>
        
    </section>
    
